{
  "name": "billie/api-php-sdk",
  "license": "proprietary",
  "type": "project",
  "description": "SDK PHP for Billie.io API",
  "homepage": "https://www.billie.io/",
  "authors": [
    {
      "name": "Marcel Barten",
      "email": "github@m-barten.de"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "5.* || ^6.3",
    "symfony/validator": "3.4.24",
    "symfony/intl": "3.4.24",
    "kamermans/guzzle-oauth2-subscriber": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "squizlabs/php_codesniffer": "^3.4",
    "friendsofphp/php-cs-fixer": "^2.14",
    "symfony/var-dumper": "^3.4",
    "ramsey/uuid": "^3.8"
  },
  "autoload": {
    "psr-4": { "Billie\\" : "lib/" }
  },
  "autoload-dev": {
    "psr-4": { "Billie\\Tests\\" : "tests/" }
  }
}
